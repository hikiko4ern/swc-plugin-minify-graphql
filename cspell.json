{
	"version": "0.2",
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
	"import": [
		"@cspell/dict-en-gb/cspell-ext.json"
	],
	"language": "en",
	"enableFiletypes": [
		"toml"
	],
	"dictionaries": [
		"css",
		"fonts",
		"node",
		"npm",
		"softwareTerms",
		"typescript",
		"rust",
		"en-gb"
	],
	"minWordLength": 3,
	"flagWords": [],
	"cache": {
		"useCache": true
	},
	"validateDirectives": true,
	"useGitignore": true,
	"words": [
		"Binaryen",
		"bumpalo",
		"dedented",
		"fnm",
		"fuzzer",
		"idents",
		"itertools",
		"memchr",
		"quasis",
		"rustup",
		"SWC",
		"whitespaces"
	],
	"ignoreWords": [
		"binstall",
		"blackbox",
		"cdylib",
		"codegen",
		"Corepack",
		"dprint",
		"fmt",
		"hikiko4ern",
		"indoc",
		"lefthook",
		"lto",
		"rustfmt",
		"sed",
		"textwrap",
		"vec",
		"xeo"
	],
	"ignorePaths": [
		"/.gitattributes",
		"/.gitignore",
		"/biome.json",
		"/cspell.json",
		"/pnpm-lock.yaml",
		"test_data",
		"afl_in"
	]
}
